package practice.TwoPointer;

import java.util.Arrays;
import java.util.stream.IntStream;

public class SumOfArrayByBruteForce {

	public static void main(String[] args) {
		        int[] numbers = {1,2,3};

		        int[] arr= {1,2,3,4,5};
		        int sum=0;
		        for(int i=0;i<arr.length;i++) {
		        	sum=sum+arr[i];
		        }
		}
	}
